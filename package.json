{
  "name": "2anki-web",
  "version": "0.1.1",
  "private": false,
  "resolve": {
    "fallback": {
      "assert": false,
      "http": false,
      "https": false,
      "stream": false,
      "url": false,
      "zlib": false
    }
  },
  "scripts": {
    "start": "cross-env REACT_SKIP_SENTRY=true vite",
    "dev": "cross-env NODE_OPTIONS=--max_old_space_size=4096 vite",
    "local": "cross-env REACT_APP_LOCALHOST=true vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "node scripts/test.js",
    "lint": "biome lint .",
    "lint:fix": "biome format . --write",
    "schema:generate-types": "npx kanel",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "@babel/core": "^7.28.0",
    "@bugsnag/js": "^7.23.0",
    "@bugsnag/plugin-react": "^8.1.1",
    "@notionhq/client": "^2.2.14",
    "@reduxjs/toolkit": "^2.6.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react-query": "^1.2.9",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^9.1.2",
    "browserslist": "^4.24.2",
    "bulma": "^1.0.4",
    "bulma-switch": "^2.0.4",
    "camelcase": "^6.2.1",
    "date-fns": "^4.1.0",
    "dotenv-expand": "^12.0.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.3.0",
    "get-notion-object-title": "^0.2.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-resolve": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "lodash": "^4.17.21",
    "postcss": "^8.5.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^13.0.1",
    "postcss-preset-env": "^10.1.1",
    "prompts": "^2.4.2",
    "react": "^19.0.0",
    "react-app-polyfill": "^3.0.0",
    "react-confetti": "^6.1.0",
    "react-cookie": "^7.2.1",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^19.0.0",
    "react-query": "^3.39.3",
    "react-redux": "^9.2.0",
    "react-refresh": "^0.11.0",
    "react-router": "^7.6.0",
    "react-router-dom": "^6.27.0",
    "resolve": "^1.22.10",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^16.0.5",
    "semver": "^7.6.3",
    "source-map-loader": "^5.0.0",
    "styled-components": "^6.1.13",
    "typescript": "^5.8.2",
    "universal-cookie": "^7.2.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@playwright/test": "^1.42.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-legacy": "^6.1.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.5.0",
    "biome": "^0.3.3",
    "cross-env": "^10.0.0",
    "dotenv": "^16.4.7",
    "jest-environment-jsdom": "^30.0.5",
    "playwright": "^1.55.0",
    "postcss-selector-parser": "^7.1.0",
    "vite": "^6.3.6",
    "vite-plugin-pwa": "^1.0.3",
    "vite-plugin-svgr": "^4.5.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}